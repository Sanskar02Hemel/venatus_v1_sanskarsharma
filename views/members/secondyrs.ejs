<%- include("../partials/header.ejs") %>
<main>
    <% if(secondyrs.length > 0) { %>
        <div class="container secondyrs">
            <div class="row">
                <% for(let secondyr of secondyrs) { %>
                <div class="col-md-4">
                    <div class="card secondyr">
                        <img src="<%= secondyr.img %> " class="card-img-top"
                            alt="PIC TEST">
                        <div class="card-body">
                            <h5 class="card-title">Name: <%= secondyr.name %></h5>
                            <p class="card-text">
                                <span>Year: <%= secondyr.year %></span><br>
                                <span>Branch: <%= secondyr.branch %></span><br>
                                <span>Email: <%= secondyr.email %></span><br>
                                <span>Gamer-Tag: <%= secondyr.tag %></span><br>
                                <span>Achievement: <%= secondyr.achievement %></span><br>
                            </p>
                            <% if (secondyr.linkedIn) { %>
                                <a href="<%= secondyr.linkedIn %>" class="btn btn-primary">LinkedIn</a>
                            <% } %>
                        </div>
                    </div>
                </div>
            <% } %>
    <% }else{ %>
            </div>
            </div>
    <div>
        No events
    </div>
    <% } %>
</main>

<%- include("../partials/footer.ejs") %>